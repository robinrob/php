<?php
//Adding items to an array using the array keyword.

$a1 = array("Copier", "Inkjet", "Laser", "Photo");

echo "a1[3]: $a1[3] <br>";

$a2 = array('copier' => "Copier & Multipurpose",
			'inkjet' => "Inkjet Printer",
			'laser'	 => "Laser Printer",
			'photo'	 => "Photographic Printer");

echo "a2['photo']: " . $a2['photo'] <br>";
?>