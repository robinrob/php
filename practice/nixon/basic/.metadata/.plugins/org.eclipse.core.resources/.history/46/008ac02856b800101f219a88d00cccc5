<?php
//Looping through a numeric array using foreach...as

$paper = array("Copier", "Inkjet", "Laser", "Photo");
$j = 0;

foreach($paper as $item)
{
	echo "$j: $item<br>";
	++$j;
}

//Looping through an associative array using foreach...as

foreach($paper as $item)
{
	echo "$j: $item<br>";
	++$j;
}
?>